"cost1 = 3.07\n"
"tax1 = cost1 * 0.06\n"
"total1 = cost1 + tax1\n"
"\n"
"print(f\"Cost:  ${cost1:5.2f}\")\n"
"print(f\"Tax:    {tax1:5.2f}\")\n"
"print(f\"-------------\")\n"
"print(f\"Total: ${total1:5.2f}\")\n"
"\n"
"cost2 = 5.07\n"
"tax2 = cost2 * 0.06\n"
